<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fz.admin.module.user.mapper.SysRoleMenuMapper">

    <resultMap id="BaseResultMap" type="com.fz.admin.module.user.model.entity.SysRoleMenu">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="roleId" column="role_id" jdbcType="BIGINT"/>
            <result property="menuId" column="menu_id" jdbcType="BIGINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,role_id,menu_id
    </sql>


    <insert id="insertBatch">
        insert into sys_role_menu(role_id, menu_id) values
        <foreach item="item" index="index" collection="roleMenus" separator=",">
            (#{item.roleId},#{item.menuId})
        </foreach>
    </insert>
</mapper>
